<Window x:Class="ComputationalMethods.Views.LeastSquaresMethod.LeastSquaresMethodDisplay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LeastSquaresMethodDisplay" Height="540" Width="960">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <Label Margin="10">a:</Label>
            <TextBox Name="aTextBox" Margin="10" Width="100"></TextBox>
            <Label Margin="10">b:</Label>
            <TextBox Name="bTextBox" Margin="10" Width="100"></TextBox>
            <Label Margin="10">epselon:</Label>
            <TextBox Name="epsTextBox" Margin="10" Width="100"></TextBox>
            <Button Name="Evaluate" Margin="10" Width="100" Click="Evaluate_Click">Evaluate</Button>
        </StackPanel>
        <StackPanel Grid.Row="1">
            <Label FontSize="18" Name="result"></Label>
        </StackPanel>
    </Grid>
</Window>
